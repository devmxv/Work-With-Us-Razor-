@page
@model WorkWithUsRazor.Pages.VacanciesList.IndexModel


<br/>
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">Book List</h2>
    </div>
    <div class="col-3">
        <a asp-page="Create" class="btn btn-info form-control text-white">Open Vacancy</a>
    </div>
    <div class="col-3">
        <a asp-page="Upsert" class="btn btn-info form-control text-white">Open Vacancy</a>
    </div>

    <div class="col-12 border p-3 m-3">
        <form method="post">
            <table class="table table-striped border">
                <thead>
                    <label asp-for="Vacancies.FirstOrDefault().Description"></label>
                </thead>
            </table>
        </form>
    </div>

</div>
